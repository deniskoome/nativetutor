APP_NAME=NativeTutor
APP_ENV=production
APP_KEY=base64:your_app_key_here
APP_DEBUG=true
APP_VERSION=1.9.7
APP_URL=https://nativetutor.com.podgin.com
LOG_CHANNEL=stack
JWT_SECRET=your_jwt_secret_here
API_KEY=your_api_key_here

# Database Settings
DB_CONNECTION=mysql
DB_HOST=your_db_host
DB_PORT=3306
DB_DATABASE=your_database_name
DB_USERNAME=your_database_user
DB_PASSWORD=your_database_password

# SMTP Settings
MAIL_DRIVER=smtp
MAIL_MAILER=smtp
MAIL_HOST=your_mail_host
MAIL_PORT=465
MAIL_USERNAME=your_email@example.com
MAIL_PASSWORD=your_email_password
MAIL_ENCRYPTION=ssl
MAIL_FROM_ADDRESS=no-reply@example.com
MAIL_FROM_NAME="Native Tutor"

# S3 / MinIO Storage
MINIO_KEY=your_minio_key
MINIO_SECRET=your_minio_secret
MINIO_REGION=your_minio_region
MINIO_BUCKET=your_minio_bucket
MINIO_ENDPOINT=your_minio_endpoint
